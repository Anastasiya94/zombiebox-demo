{{$ demo.widgets.templates.keyboardExtended.keyboardExtended }}

<div class="w-keyboard-extended">
	<!--ABC english keyboard-->
	<div class="w-keyboard-extended__layout _english zb-clear" data-component="{{% demo.widgets.KeyboardExtendedLayout, {type: demo.widgets.KeyboardExtended.Types.ABC, lang: demo.widgets.KeyboardExtended.Lang.EN}, layoutAbcEn }}">
		<div class="w-keyboard-extended__line">
			<div class="w-keyboard-extended__center">
				{{ "q w e r t y u i o p".split(" ").forEach(function(sym) { }}
					<div class="w-keyboard-extended-item" data-export-id="{{@itemsAbcEn[]}}">{{=sym}}</div>
				{{ }); }}
			</div>
		</div>
		<div class="w-keyboard-extended__line">
			<div class="w-keyboard-extended__center">
				{{ "a s d f g h j k l".split(" ").forEach(function(sym) { }}
					<div class="w-keyboard-extended-item" data-export-id="{{@itemsAbcEn[]}}">{{=sym}}</div>
				{{ }); }}
			</div>
		</div>
		<div class="w-keyboard-extended__line">
			<div class="w-keyboard-extended__center">
				<div class="w-keyboard-extended-item _caps" data-export-id="{{@itemsAbcEn[]}}" data-keyboard-action="{{=demo.widgets.KeyboardExtended.Actions.CAPS}}"></div>
				{{ "z x c v b n m".split(" ").forEach(function(sym) { }}
					<div class="w-keyboard-extended-item" data-export-id="{{@itemsAbcEn[]}}">{{=sym}}</div>
				{{ }); }}
				<div class="w-keyboard-extended-item _arrow _left" data-export-id="{{@itemsAbcEn[]}}" data-keyboard-action="{{=demo.widgets.KeyboardExtended.Actions.LEFT}}"></div>
				<div class="w-keyboard-extended-item _arrow _right" data-export-id="{{@itemsAbcEn[]}}" data-keyboard-action="{{=demo.widgets.KeyboardExtended.Actions.RIGHT}}"></div>
			</div>
		</div>
		<div class="w-keyboard-extended__line">
			<div class="w-keyboard-extended__center">
				<div class="w-keyboard-extended-item _symbols" data-export-id="{{@itemsAbcEn[]}}" data-keyboard-action="{{=demo.widgets.KeyboardExtended.Actions.TYPE_NUM}}">?123</div>
				<div class="w-keyboard-extended-item _switch-lang _green" data-export-id="{{@itemsAbcEn[]}}" data-keyboard-action="{{=demo.widgets.KeyboardExtended.Actions.LANG_RU}}"></div>
				<div class="w-keyboard-extended-item" data-export-id="{{@itemsAbcEn[]}}">,</div>
				<div class="w-keyboard-extended-item _whitespace _yellow" data-export-id="{{@itemsAbcEn[]}}" data-keyboard-action="{{=demo.widgets.KeyboardExtended.Actions.SPACE}}">Space</div>
				<div class="w-keyboard-extended-item" data-export-id="{{@itemsAbcEn[]}}">.</div>
				<div class="w-keyboard-extended-item _backspace _blue" data-export-id="{{@itemsAbcEn[]}}" data-keyboard-action="{{=demo.widgets.KeyboardExtended.Actions.BACKSPACE}}">.</div>
				<div class="w-keyboard-extended-item _enter _red" data-export-id="{{@itemsAbcEn[]}}" data-keyboard-action="{{=demo.widgets.KeyboardExtended.Actions.ENTER}}">Enter</div>
			</div>
		</div>
	</div>

